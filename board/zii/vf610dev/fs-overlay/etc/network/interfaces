auto lo
iface lo inet loopback

auto eth0
iface eth0 inet static
      address 10.0.0.2
      netmask 255.0.0.0

auto eth1
iface eth1 inet manual
    pre-up ifconfig $IFACE up
    post-down ifconfig $IFACE down

auto lan0
iface lan0 inet manual
    pre-up ifconfig $IFACE up
    post-down ifconfig $IFACE down

